   "execution_count": 4,
   "id": "ff39b38c",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-04-01T05:40:59.302227Z",
     "iopub.status.busy": "2025-04-01T05:40:59.301835Z",
