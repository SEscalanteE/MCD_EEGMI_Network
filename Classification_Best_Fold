     "shell.execute_reply": "2025-04-01T05:41:08.803813Z"
    },
    "id": "yE1sbHYQVbBq",
    "papermill": {
     "duration": 9.525219,
     "end_time": "2025-04-01T05:41:08.807720",
     "exception": false,
     "start_time": "2025-04-01T05:40:59.282501",
     "status": "completed"
