   "cell_type": "code",
   "execution_count": 4,
   "id": "c5de5d29",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-12-12T14:12:07.804434Z",
     "iopub.status.busy": "2024-12-12T14:12:07.804065Z",
