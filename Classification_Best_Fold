   "cell_type": "code",
   "execution_count": 7,
   "id": "ccfaafef",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-05-21T13:24:56.101594Z",
     "iopub.status.busy": "2025-05-21T13:24:56.101031Z",
     "iopub.status.idle": "2025-05-21T14:31:10.255116Z",
     "shell.execute_reply": "2025-05-21T14:31:10.253116Z"
    },
    "id": "Ymqd_W21y3NK",
    "outputId": "5ca97a2f-f57c-46ee-8f53-f00181ccea90",
    "papermill": {
     "duration": 3974.479888,
