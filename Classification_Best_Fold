   "cell_type": "code",
   "execution_count": 6,
   "id": "db447808",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-12-11T22:43:55.863007Z",
     "iopub.status.busy": "2024-12-11T22:43:55.862474Z",
     "iopub.status.idle": "2024-12-11T22:43:55.885796Z",
     "shell.execute_reply": "2024-12-11T22:43:55.883166Z"
