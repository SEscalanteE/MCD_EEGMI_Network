   "cell_type": "code",
   "execution_count": 7,
   "id": "7b90f12a",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-12-27T14:13:02.744757Z",
     "iopub.status.busy": "2024-12-27T14:13:02.743778Z",
     "iopub.status.idle": "2024-12-27T14:35:36.782045Z",
     "shell.execute_reply": "2024-12-27T14:35:36.780556Z"
    },
    "id": "Ymqd_W21y3NK",
    "outputId": "5ca97a2f-f57c-46ee-8f53-f00181ccea90",
